{
  "version": 2,
  "routes": [
    {
      "src": "/$",
      "dest": "/index.html"
    },
    {
      "src": "/jsonViewer$",
      "status": 301,
      "headers": {
        "Location": "/jsonViewer/"
      }
    },
    {
      "src": "/jsonTool$",
      "status": 301,
      "headers": {
        "Location": "/jsonTool/"
      }
    }
  ]
}